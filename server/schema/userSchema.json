 {
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "email": {
        "$id": "/properties/email",
        "type": "string",
        "title": "The Email Schema ",
        "default": "",
        "examples": [
          "snanilim@mail.com"
        ]
      },
      "userName": {
        "$id": "/properties/userName",
        "type": "string",
        "title": "The Username Schema ",
        "default": "",
        "examples": [
          "admin"
        ]
      },
      "password": {
        "$id": "/properties/password",
        "type": "string",
        "title": "The Password Schema ",
        "default": "",
        "examples": [
          "admin@123"
        ]
      }
    },
    "required": [
      "email",
      "userName",
      "password"
    ]
  }